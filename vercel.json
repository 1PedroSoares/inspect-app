{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        },
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(css|js|images|fonts|favicon\\.ico|robots\\.txt)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "<COLOQUE_SUA_APP_KEY_NO_PAINEL_DA_VERCEL>",
        "APP_URL": "<COLOQUE_A_URL_DO_PROJETO_AQUI_NO_PAINEL>",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "<CONFIGURE_NO_PAINEL_DA_VERCEL>",
        "DB_PORT": "3306",
        "DB_DATABASE": "<CONFIGURE_NO_PAINEL_DA_VERCEL>",
        "DB_USERNAME": "<CONFIGURE_NO_PAINEL_DA_VERCEL>",
        "DB_PASSWORD": "<CONFIGURE_NO_PAINEL_DA_VERCEL>",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr"
    }
}