{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "/public/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "/api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "/build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(css|js|images|fonts|favicon\\.ico|robots\\.txt)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.php"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://<SEU_DOMINIO_VERCEL>.vercel.app",
        "APP_KEY": "<SUA_APP_KEY_GERADA>",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "<SEU_DB_HOST>",
        "DB_PORT": "<SEU_DB_PORT>",
        "DB_DATABASE": "<SEU_DB_DATABASE>",
        "DB_USERNAME": "<SEU_DB_USERNAME>",
        "DB_PASSWORD": "<SEU_DB_PASSWORD>",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array",
        "QUEUE_CONNECTION": "sync",
        "LOG_CHANNEL": "stderr"
    }
}